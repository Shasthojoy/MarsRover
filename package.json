{
  "name": "MarsRover",
  "description": "An action game where you poison zombie nazis on Mars.",
  "readme": "README.md",
  "repository": "https://github.com/kennedyr/MarsRover",
  "version": "0.0.1",
  "engines": {
    "node": "*",
    "npm": "*"
  },
  "dependencies": {
    "bower": "*",
    "browserify": "*",
    "browserify-shim": "*",
    "coffee-script": "*",
    "coffeeify": "*",
    "connect": "*",
    "gulp": "*",
    "gulp-install": "*",
    "gulp-less": "*",
    "gulp-notify": "*",
    "gulp-open": "*",
    "gulp-util": "*",
    "pretty-hrtime": "*",
    "vinyl-source-stream": "*",
    "yargs": "3.11.x"
  },
  "browser": {
    "phaser": "./src/bower_components/phaser-official/build/phaser.min.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      "coffeeify"
    ]
  },
  "browserify-shim": {
    "phaser": "Phaser"
  },
  "devDependencies": {
    "browser-sync": "*",
    "watchify": "*"
  },
  "scripts": {
    "post-install": "node ./node_modules/bower/bin/bower install",
    "start": "node ./node_modules/gulp/bin/gulp.js",
    "build": "node ./node_modules/gulp/bin/gulp.js build",
    "production": "node ./node_modules/gulp/bin/gulp.js --production"
  }
}
